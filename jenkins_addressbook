pipeline {
    agent {
        node {
            label 'Win_NET'
        }
    }

    tools {

        maven 'mymaven'
        jdk 'myjava'
    }

    stages {
        stage ('Checkout code from SCM') {
            steps {

                git branch: 'master', url: 'https://github.com/shrutileena/addressbook.git'

            }
        }
    }

    stages {
        stage ('Code Compile') {

            steps {
                bat """
                mvn Compile
                """ 
            }

        }
    }
}
